@article{Kerrebrock2012,
    abstract = {A general theory is proposed for the small disturbance field in strongly swirling flows in turbomachines. In contrast to the situation in nonswirling flows, vorticity, pressure, and entropy fields are not independent. Shear disturbances are not purely convected but rather propagate slowly in flows stable in the sense of Rayleigh, and are unstable in flows approaching free vortices. In the course of the propagation, there is an interchange between radial and tangential velocity perturbations. General conditions for propagation of pressure modes in the strongly swirling flow show the effects of rotation to be small if the frequency is large compared to base rotational frequency. Oscillatory shear flows have been observed in experiments in the MIT Blowdown Compressor. © 1977 American Institute of Aeronautics and Astronautics, Inc. All rights reserved.},
    author = {J. L. Kerrebrock},
    doi = {10.2514/3.7370},
    issn = {00011452},
    issue = {6},
    journal = {https://doi.org/10.2514/3.7370},
    keywords = {Aircraft Wings,Entropy,Incompressible Flow,Kinematic Viscosity,Mach Number,Radial Velocity,Stagnation Temperature,Turbine Nozzles,Turbomachinery,Vortex Flow},
    month = {5},
    pages = {794-803},
    title = {Small disturbances in turbomachine annuli with swirl},
    volume = {15},
    url = {https://arc.aiaa.org/doi/10.2514/3.7370},
    year = {2012},
}
@article{KERREBROCK1974,
    abstract = {IN THE STRONGLY ROTATING FLOWS OF TURBOMACHINES, THE BEHAVIOUR OF SMALL SHEAR, PRESSURE AND ENTROPY DISTURBANCES, COMPRISING A GENERAL SMALL DISTURBANCE FIELD, CAN BE MARKEDLY DIFFERENT FROM THAT IN A NONROTATING EXTERNAL FLOW.THE PURPOSE OF THIS WORK IS TO ELUCIDATE THIS BEHAVIOUR AS A BASIS FOR BETTER UNDERSTANDING OF THE SOUND GENERATION AND LOSS MECHANISMS IN TURBOMACHINES.TO THIS END, A GENERAL FORMULATION OF SMALL DISTURBANCES IN STRONGLY ROTATING FLOW IS PRESENTED, AND A GENERAL SOLUTION IN SEPARATED AXIAL, AZIMUTHAL AND RADIAL COORDINATES IS OBTAIWED.FOR THE SPECIAL CASE OF ISENTROPIC FLOW, CLOSED SOLUTIONS ARE OBTAINED FOR WHEEL FLOW AND FOR FREE VORTEX FLOW.},
